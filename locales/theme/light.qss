QSpinBox[class="OutcomeView"]::up-button, 
QSpinBox[class="OutcomeView"]::down-button {
    background-color: #EF9A9A;
    border: none;
    border-radius: 3px;
    width: 20px;
}

QSpinBox[class="OutcomeView"]::up-button:hover, 
QSpinBox[class="OutcomeView"]::down-button:hover {
    background-color: #E57373;
}

QStackedWidget {
    background-color: #FFFFFF;
}

/* Statistic View Styles */
#iconDesignStatistic {
    QComboBox {
        background-color: transparent;
        width: 60px;
        height: 22px;
        padding: 0px;
        border: none;
        text-align: center;
    }

    QComboBox::drop-down {
        width: 60px;
    }

    QComboBox::down-arrow {
        image: url(img/icon/database.svg);
        width: 60px;
        height: 60px;
    }

    QComboBox::lineEdit {
        border: none;
        background: transparent;
        padding: 100px;
        color: transparent;
    }

    QComboBox QAbstractItemView {
        min-width: 150px;
        max-width: 300px;
        font-size: 14px;
        padding: 10px;
    }

    QComboBox QAbstractItemView::item {
        padding-left: 10px;
        padding-right: 10px;
        height: 40px;
    }

    QComboBox::item:selected {
        background-color: #BBDEFB;
        color: #333333;
    }

    QComboBox::editable {
        background: transparent;
        border: none;
        padding: 0px;
        color: transparent;
    }
}

#textDesignStatistic {
    QLabel {
        font-family: 'Arimo';
        font-size: 16px;
        color: #333333;
        font-weight: bold;
        text-align: center;
        border: none;
    }

    #changeValue {
        font-family: 'Arimo';
        font-size: 10px;
        color: #666666;
        font-weight: bold;
    }
}

#chartDesignStatistic {
    QFrame {
        border: 10px solid #B0BEC5;
        padding: 20px;
        background-color: #FFFFFF;
        border-radius: 15px;
    }
}

#DesignStatistic {
    QFrame {
        border: 10px solid #B0BEC5;
        padding: 20px;
        background-color: #FFFFFF;
        border-radius: 15px;
    }
}

#buttonDesignStatistic {
    QPushButton {
        background-color: #E0E0E0;
        border-radius: 10px;
        padding: 5px;
        font-weight: bold;
        color: #333333;
    }

    QPushButton:hover {
        background-color: #BDBDBD;
    }

    QPushButton:pressed {
        background-color: #9E9E9E;
    }
}
